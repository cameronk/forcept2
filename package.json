{
  "name": "forcept",
  "version": "2.0.0",
  "private": true,
  "main": "start.js",
  "scripts": {
    "build": "rm -rf ./dist && webpack --progress --config webpack.config.production.js",
    "dev": "node run-dev.js",
    "lint": "eslint ./*.js ./**/*.js",
    "start": "node run-prod.js",
    "configure": "node bin/configure.js",
    "db:migrate:prod": "node_modules/.bin/sequelize db:migrate --env production",
    "db:seed:prod": "node_modules/.bin/sequelize db:seed:all --env production",
    "db:migrate:dev": "node_modules/.bin/sequelize db:migrate",
    "db:seed:dev": "node_modules/.bin/sequelize db:seed:all",
    "db:setup": "npm run db:migrate:prod && npm run db:seed:prod",
    "db:setup:dev": "npm run db:migrate:dev && npm run db:seed:dev",
    "db:seed:rollback": "sequelize db:seed:undo:all"
  },
